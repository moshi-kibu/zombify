
<div id = 'login'>
	<h1>login!</h1>

	<%= form_tag sessions_path do %>
	  <%= label_tag :email %>
	  <%= email_field_tag :email %>
	  <%= label_tag :password %>
	  <%= password_field_tag :password%>
	  <%= submit_tag("Log In") %><span class = "back_link" ><a href="/">     return to home</a></span>
	<% end %>
</div>